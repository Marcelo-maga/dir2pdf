{
  "name": "dir2pdf",
  "version": "1.5.1",
  "description": "A simple module to turn a folder images into a pdf file",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix", 
      "npm run test --bail", 
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Marcelo-maga/dir2pdf.git"
  },
  "author": "Marcelo-maga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Marcelo-maga/dir2pdf/issues"
  },
  "homepage": "https://github.com/Marcelo-maga/dir2pdf#readme",
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.0",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "pdf",
    "image",
    "conveter"
  ]
}
